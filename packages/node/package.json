{
  "name": "@nazarkulyk/eslint-config-node",
  "version": "5.0.0",
  "description": "Pluggable eslint config for Node.js that you can import, extend and override",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "js-yaml index.yaml > index.json && js-yaml style-guide/index.yaml > style-guide/index.json",
    "clean": "rimraf index.json style-guide/index.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nazarkulyk/eslint-config-esnext.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig",
    "node",
    "node.js"
  ],
  "author": "nazar.kulyk@ibsolution.de",
  "license": "ISC",
  "homepage": "https://github.com/nazarkulyk/eslint-config-esnext/tree/master/packages/node#readme",
  "dependencies": {
    "eslint": "^8.26.0",
    "@nazarkulyk/eslint-config-esnext": "^5.0.0"
  },
  "devDependencies": {
    "js-yaml": "^4.1.0",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "eslint": ">=7.5.0 <=9"
  }
}
